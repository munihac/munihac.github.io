{
  "name": "bootstrap-customized",
  "version": "0.0.1",
  "private": true,
  "description": "Customizing the color scheme for MuniHac",
  "scripts": {
    "build": "npm run css",
    "css-compile": "node-sass --include-path node_modules --output-style expanded --source-map true --source-map-contents true --precision 6 scss -o .",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace styles.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "test": "npm run css-lint && npm run css"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.4.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.2",
    "serve": "^11.3.2",
    "stylelint": "^13.12.0",
    "stylelint-config-twbs-bootstrap": "^2.1.0"
  }
}
